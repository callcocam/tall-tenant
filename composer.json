{
    "name": "callcocam/tall-tenant",
    "type": "package",
    "description": "Laravel package for multi-tenant with one or more databases, Pacote laravel para mult-tenant com um ou mais database.",
    "keywords": ["framework", "laravel", "livewire", "alpinejs", "tailwindcss"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "callcocam/tall-sluggable": "dev-main"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Tall\\Tenant\\": "src"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Tall\\Tenant\\Providers\\TenantServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
